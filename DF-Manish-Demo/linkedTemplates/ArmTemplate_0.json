{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DF-Manish-Demo"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"linkedServiceAzureSQL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedServiceAzureSQL'"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzureBlobStorage1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('AzureBlobStorage1_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/linkedServiceAzureSQL')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": "[parameters('linkedServiceAzureSQL_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureSqlTabledatabase')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "table name LTC_CLAIM",
				"linkedServiceName": {
					"referenceName": "linkedServiceAzureSQL",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "PRNT_CONTRACT_NUM",
						"type": "nvarchar"
					},
					{
						"name": "DOS",
						"type": "nvarchar"
					},
					{
						"name": "MA_NUM",
						"type": "nvarchar"
					},
					{
						"name": "CARRIER_CD",
						"type": "nvarchar"
					},
					{
						"name": "POLICY_NUM",
						"type": "nvarchar"
					},
					{
						"name": "CARRIER_NAME",
						"type": "nvarchar"
					},
					{
						"name": "CLAIM_FORM_CD",
						"type": "nvarchar"
					},
					{
						"name": "BASE_ICN_NUM",
						"type": "nvarchar"
					},
					{
						"name": "LINE_NBR",
						"type": "nvarchar"
					},
					{
						"name": "CLAIM_TYPE_CD",
						"type": "nvarchar"
					},
					{
						"name": "CONTRACT_NUM",
						"type": "nvarchar"
					},
					{
						"name": "MA_PAID_AMT",
						"type": "nvarchar"
					},
					{
						"name": "PROVIDER_ID",
						"type": "nvarchar"
					},
					{
						"name": "RESUBMISSION_IND",
						"type": "nvarchar"
					}
				],
				"typeProperties": {
					"schema": "dbo",
					"table": "LTC_CLAIM"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/linkedServiceAzureSQL')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/DelimitedText1')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureBlobStorage1",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"container": "input"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "PRNT_CONTRACT_NUM",
						"type": "String"
					},
					{
						"name": "DOS",
						"type": "String"
					},
					{
						"name": "MA_NUM",
						"type": "String"
					},
					{
						"name": "CARRIER_CD",
						"type": "String"
					},
					{
						"name": "POLICY_NUM",
						"type": "String"
					},
					{
						"name": "CARRIER_NAME",
						"type": "String"
					},
					{
						"name": "CLAIM_FORM_CD",
						"type": "String"
					},
					{
						"name": "BASE_ICN_NUM",
						"type": "String"
					},
					{
						"name": "LINE_NBR",
						"type": "String"
					},
					{
						"name": "CLAIM_TYPE_CD",
						"type": "String"
					},
					{
						"name": "CONTRACT_NUM",
						"type": "String"
					},
					{
						"name": "MA_PAID_AMT",
						"type": "String"
					},
					{
						"name": "PROVIDER_ID",
						"type": "String"
					},
					{
						"name": "RESUBMISSION_IND",
						"type": "String"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureBlobStorage1')]"
			]
		}
	]
}